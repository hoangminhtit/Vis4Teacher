
# Django Settings
SECRET_KEY=your-super-secret-key-here-change-in-production
DEBUG=True

# Database (PostgreSQL/Supabase)
database=your_database_name
user=your_database_user
password=your_database_password
host=your_database_host
port=5432

# Metabase Configuration
# Khi chạy local: http://localhost:3000
# Khi chạy Docker: http://host.docker.internal:3000 (để truy cập Metabase trên host)
METABASE_URL=http://localhost:3000
METABASE_SECRET_KEY=your_metabase_secret_key
METABASE_DASHBOARD_ID=2
METABASE_STUDENT_DASHBOARD_ID=3

# CORS (comma separated for multiple origins)
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
